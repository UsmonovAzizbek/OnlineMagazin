<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>org.example.controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>client</servlet-name>
        <servlet-class>org.example.controller.ClientList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>client</servlet-name>
        <url-pattern>/client_list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>product</servlet-name>
        <servlet-class>org.example.controller.ProductList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>product</servlet-name>
        <url-pattern>/product_list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>category</servlet-name>
        <servlet-class>org.example.controller.Category</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>category</servlet-name>
        <url-pattern>/category</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>org.example.controller.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>org.example.controller.Update</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>org.example.controller.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register_p</servlet-name>
        <servlet-class>org.example.controller.RegisterP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register_p</servlet-name>
        <url-pattern>/register_p</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>update_p</servlet-name>
        <servlet-class>org.example.controller.UpdateP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update_p</servlet-name>
        <url-pattern>/update_p</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delete_p</servlet-name>
        <servlet-class>org.example.controller.DeleteP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete_p</servlet-name>
        <url-pattern>/delete_p</url-pattern>
    </servlet-mapping>

</web-app>